.publish {
  position: relative;
}
.publish .overlay {
  position: absolute;
  z-index: 10;
}
.publish .wrapper {
  width: 500rpx;
  height: 300rpx;
  background-color: #fff;
  border-radius: 30rpx;
  margin: 50%;
  transform: translateX(-50%);
}
.publish .wrapper .contant {
  text-align: center;
  display: flex;
  flex-direction: column;
  padding: 50rpx;
  font-size: 40rpx;
}
.publish .wrapper .msg-contant {
  text-align: center;
  padding: 50rpx;
  font-size: 40rpx;
}
.publish .msg-wrapper {
  width: 500rpx;
  height: 300rpx;
  background-color: #fff;
  border-radius: 30rpx;
  margin: 50%;
  transform: translateX(-50%);
  display: flex;
  justify-content: center;
  align-items: center;
}
.publish .msg-wrapper .msg-contant {
  text-align: center;
  padding: 50rpx;
  font-size: 40rpx;
}
.publish .deliveryTime {
  width: 100%;
  display: flex;
  justify-content: start;
  align-items: center;
  font-size: 28rpx;
  padding: 32rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #f5f5f5;
}
.publish .deliveryTime .delivery-time-left {
  width: 29%;
  color: #6a6a6a;
  display: flex;
  justify-content: start;
  margin-left: -28rpx;
}
.publish .deliveryTime .delivery-time-right {
  width: 70%;
  color: #dadada;
  display: flex;
  justify-content: space-between;
}
.publish .sex-style {
  width: 100%;
  font-size: 28rpx;
  padding: 32rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #f5f5f5;
  display: flex;
  justify-content: start;
  align-items: center;
}
.publish .sex-style .sex-style-left {
  width: 29%;
  display: flex;
  justify-content: start;
  margin-left: -28rpx;
}
.publish .sex-style .sex-style-right {
  width: 70%;
  display: flex;
  justify-content: space-around;
}
.publish .sex-style .sex-style-right view {
  display: flex;
  justify-content: start;
}